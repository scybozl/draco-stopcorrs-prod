#  CORE_SCALE VAR{sqr(PRmss)}{sqr(PRmss)}{sqr(PRmss)};
  # REWEIGHT_SPLITTING_ALPHAS_SCALES 0; REWEIGHT_MAXEM      0;
  # REWEIGHT_SPLITTING_PDF_SCALES    0; REWEIGHT_MCATNLO_EM 0;

#  ORDER_ALPHAS 1;
