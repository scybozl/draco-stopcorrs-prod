  PRtgerr:=0.1;
  PRsteps:=100000;    # 150000; 100000; 125000; 50000; 200000; 75000;

  PRparamcard:=tL.190-M1.20-mt.173.1.dat;    # m3L; m3R; .20.; .50.; .150.;
  PRmss:=190.0
  PRcim:=20.0
  PRran:=428
  PRshower:=CSS

  TMASS:=173.1;

  FSF:=1.; RSF:=1.; QSF:=1.0; PRfsas:=1.0; PRisas:=0.5;     # cnt
